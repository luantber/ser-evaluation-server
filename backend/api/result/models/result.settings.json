{
  "kind": "collectionType",
  "collectionName": "results",
  "info": {
    "name": "Result"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "results"
    },
    "date": {
      "type": "datetime"
    },
    "metrics": {
      "type": "json"
    },
    "challenge": {
      "via": "results",
      "model": "challenge"
    },
    "name": {
      "type": "string"
    }
  }
}
